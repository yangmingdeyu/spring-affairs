<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qf.dao.AccoutnMapper">

    <!-- 根据用户id, 金钱更新用户信息 -->
    <update id="update">
        update account set money = money+ #{money} where id = #{id}
    </update>

</mapper>